{{ form_start(form, {
  attr: {
    'data-turbo-frame': 'upload_frame'
  }
}) }}
<div class="form-control mb-4">
	{{ form_label(form.nom) }}
	{{ form_widget(form.nom, {'attr': {'class': 'input input-bordered w-full'}}) }}
</div>
<div class="form-control mb-4">
	{{ form_label(form.pdf) }}
	{{ form_widget(form.pdf, {'attr': {'class': 'file-input file-input-bordered w-full'}}) }}
</div>
<button class="btn btn-primary">Uploader</button>
{{ form_end(form) }}
